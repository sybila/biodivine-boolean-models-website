// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Model {
  id             String   @id @default(uuid())
  name           String
  urlPublication String
  urlModel       String
  keywords       String[]
  variables      Int
  inputs         Int
  regulations    Int
  notes          String
  bib            String
  modelData      Bytes
}
